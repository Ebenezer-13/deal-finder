{
  "name": "deal-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.15.1"
  },
  "scripts": {
    "start": "cd client react-scripts start",
    "build": "cd client react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev":
      "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
